    "# Definições\n",
    "BASE_POS = [\'NOUN\', \'VERB\', \'ADJ\', \'ADV\', \'ADP\', \'PROPN\']\n",
    "\n",
    "def calcular_icoer(frases):\n",
    "    \"\"\"\n",
    "    Calcula o Índice de Coerência Informacional (ICOER) para uma lista de frases.\n",
    "    \n",
    "    O ICOER é calculado com base na entropia da distribuição de Partes do Discurso (POS tags).\n",
    "    Um valor mais alto indica maior coerência informacional.\n",
    "    \n",
    "    Args:\n",
    "        frases (list): Lista de strings contendo as frases a serem analisadas.\n",
    "    \n",
    "    Returns:\n",
    "        pd.DataFrame: DataFrame com colunas narraton, frase, n_palavras, tags e icoer.\n",
    "                      Inclui uma linha de resumo com médias gerais e tags agregados.\n",
    "    \"\"\"\n",
    "    # Se a lista de frases estiver vazia, retorna um DataFrame vazio com as colunas corretas\n",
    "    if not frases:\n",
    "        return pd.DataFrame(columns=[\'narraton\', \'frase\', \'n_palavras\', \'tags\', \'icoer\'])\n",
    "    \n",
    "    # Processa as frases em lote usando nlp.pipe para eficiência\n",
    "    docs = list(nlp.pipe(frases))\n",
    "    registros = []\n",
    "\n",
    "    for i, doc in enumerate(docs, start=1):\n",
    "        # Ignorar frases vazias ou muito curtas que são apenas fragmentos\n",
    "        # Considera uma frase válida se tiver pelo menos 3 tokens ou se contiver letras/números\n",
    "        if not doc.text.strip() or len(doc) < 3 or not any(t.is_alpha or t.is_digit for t in doc):\n",
    "            continue\n",
    "            \n",
    "        counts = Counter(t.pos_ for t in doc) # Conta a frequência de cada POS tag na frase\n",
    "        total = sum(counts.values()) # Número total de tokens com POS tag\n",
    "        \n",
    "        # Vetor de proporções para as POS tags base\n",
    "        vetor = [counts.get(pos, 0)/total if total > 0 else 0 for pos in BASE_POS]\n",
    "        \n",
    "        # Filtra proporções maiores que zero para cálculo da entropia\n",
    "        probs = np.array([v for v in vetor if v > 0])\n",
    "        \n",
    "        # Calcula a entropia de Shannon\n",
    "        ent = -np.sum(probs * np.log2(probs)) if probs.size else 0\n",
    "        \n",
    "        # Entropia máxima: log2 do número de categorias POS base\n",
    "        # Isso garante que a normalização seja consistente, independentemente das tags presentes na frase\n",
    "        ent_max = np.log2(len(BASE_POS)) if len(BASE_POS) > 1 else 1 # Evita log2(0) ou log2(1) para uma única categoria\n",
    "        \n",
    "        # Calcula o ICOER, garantindo que seja não-negativo\n",
    "        icoer = round(max(0, 1 - (ent/ent_max if ent_max > 0 else 0)), 4)\n",
    "        \n",
    "        registros.append({\n",
    "            \'narraton\': i,\n",
    "            \'frase\': doc.text,\n",
    "            \'n_palavras\': len(doc),\n",
    "            \'tags\': dict(counts),\n",
    "            \'icoer\': icoer\n",
    "        })\n",
    "\n",
    "    df = pd.DataFrame(registros)\n",
    "    \n",
    "    # Se o DataFrame estiver vazio após a filtragem, retorna um DataFrame de resumo indicando isso\n",
    "    if df.empty:\n",
    "        return pd.DataFrame([{\n",
    "            \'narraton\': 0,\n",
    "            \'frase\': \'NENHUMA FRASE VÁLIDA PARA ANÁLISE\',\n",
    "            \'n_palavras\': 0,\n",
    "            \'tags\': {},\n",
    "            \'icoer\': 0\n",
    "        }])\n",
    "    \n",
    "    # Linha de resumo com tags agregados (média das contagens de POS tags por categoria)\n",
    "    tags_summary = {}\n",
    "    for pos in BASE_POS:\n",
    "        # Coleta as contagens para cada POS tag em todas as frases válidas\n",
    "        pos_counts = [r[\'tags\'].get(pos, 0) for r in registros]\n",
    "        tags_summary[pos] = round(np.mean(pos_counts), 2) if pos_counts else 0\n",
    "        \n",
    "    resumo = pd.DataFrame([{\n",
    "        \'narraton\': 0,\n",
    "        \'frase\': \'MÉDIA GERAL\',\n",
    "        \'n_palavras\': round(df[\'n_palavras\'].mean(), 2),\n",
    "        \'tags\': tags_summary,\n",
    "        \'icoer\': round(df[\'icoer\'].mean(), 4)\n",
    "    }])\n",
    "    \n",
    "    # Concatena o resumo com os resultados detalhados e reordena as colunas\n",
    "    return pd.concat([resumo, df], ignore_index=True)[[\'narraton\',\'frase\',\'n_palavras\',\'tags\',\'icoer\']]"
   ]
  },
