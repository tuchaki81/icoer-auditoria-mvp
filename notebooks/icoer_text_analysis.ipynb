    "# Função para executar análise, exportar CSV e mostrar visualização\n",
    "def on_button_click(b):\n",
    "    with output:\n",
    "        output.clear_output() # Limpa a saída anterior\n",
    "        \n",
    "        # Divide o texto de entrada em frases, removendo espaços em branco e entradas vazias\n",
    "        frases = [s.strip() for s in text_input.value.split(\".\") if s.strip()]\n",
    "        \n",
    "        # Verifica se há frases válidas após a divisão e filtragem inicial\n",
    "        if not frases:\n",
    "            print(\"Erro: Nenhuma frase válida fornecida para análise. Por favor, digite algumas frases.\")\n",
    "            return\n",
    "        \n",
    "        # Calcula o ICOER para as frases fornecidas\n",
    "        df_result = calcular_icoer(frases)\n",
    "        \n",
    "        # Exibe o DataFrame de resultados\n",
    "        display(df_result)\n",
    "        \n",
    "        # Exportar CSV com tratamento de erro\n",
    "        csv_filename = \"icoer_narratons.csv\"\n",
    "        try:\n",
    "            df_result.to_csv(csv_filename, index=False)\n",
    "            print(f\"Arquivo \\'{csv_filename}\\' salvo com sucesso.\")\n",
    "        except Exception as e:\n",
    "            print(f\"Erro ao salvar CSV: {e}\")\n",
    "            \n",
    "        # Visualização do ICOER por frase (exclui a linha de resumo)\n",
    "        # Garante que há dados para plotar e que a primeira linha não é o resumo\n",
    "        if not df_result.empty and df_result[\'narraton\'].iloc[0] != 0:\n",
    "            # Filtra a linha de resumo para o gráfico\n",
    "            df_plot = df_result[df_result[\'narraton\'] != 0]\n",
    "            \n",
    "            plt.figure(figsize=(12, 7))\n",
    "            plt.bar(df_plot[\'narraton\'], df_plot[\'icoer\'], color=\'skyblue\')\n",
    "            plt.xlabel(\'Narraton (Número da Frase)\')\n",
    "            plt.ylabel(\'ICOER\')\n",
    "            plt.title(\'Índice de Coerência Informacional por Frase\')\n",
    "            plt.xticks(df_plot[\'narraton\']) # Garante que todos os números de narraton sejam exibidos no eixo X\n",
    "            plt.grid(axis=\'y\', linestyle=\'--\', alpha=0.7) # Adiciona grade no eixo Y\n",
    "            plt.tight_layout() # Ajusta o layout para evitar sobreposição\n",
    "            plt.show()\n",
    "        elif df_result.empty or df_result[\'narraton\'].iloc[0] == 0 and df_result[\'frase\'].iloc[0] == \'NENHUMA FRASE VÁLIDA PARA ANÁLISE\':\n",
    "            print(\"Não há dados válidos para gerar o gráfico.\")\n",
    "\n",
    "# Associa a função on_button_click ao evento de clique do botão\n",
    "button.on_click(on_button_click)"
   ]
